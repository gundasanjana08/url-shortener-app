datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Link {
  id          Int      @id @default(autoincrement())
  slug        String   @unique
  originalUrl String
  clicks      Int      @default(0)
  createdAt   DateTime @default(now())
}


